{
	"name": "@oracle/suitecloud-cli",
	"version": "1.0.4",
	"license": "UPL-1.0",
	"description": "SuiteCloud CLI for Node.js",
	"keywords": [
		"NetSuite",
		"SuiteCloud",
		"SDF"
	],
	"publishConfig": {
		"access": "public"
	},
	"bugs": {
		"url": "https://github.com/oracle/netsuite-suitecloud-sdk/issues"
	},
	"repository": "https://github.com/oracle/netsuite-suitecloud-sdk/tree/master/packages/node-cli",
	"files": [
		"dist"
	],
	"main": "./dist/suitecloud.js",
	"preferGlobal": true,
	"sdkDownloadUrl": "https://system.netsuite.com/download/suitecloud-sdk/cli/java/latest",
	"bin": {
		"suitecloud": "./dist/suitecloud.js"
	},
	"dependencies": {
		"@oracle/suitecloud-cli-localserver-command": "^1.0.3",
		"chalk": "^4.0.0",
		"cli-spinner": "^0.2.10",
		"commander": "^5.1.0",
		"inquirer": "^7.1.0",
		"request-promise-native": "^1.0.7",
		"vm2": "^3.9.2",
		"xml2js": "^0.4.19"
	},
	"devDependencies": {
		"@types/inquirer": "^6.5.0",
		"@types/jest": "^26.0.4",
		"@types/node": "^12.11.7",
		"copyfiles": "^2.2.0",
		"jest": "^26.0.1",
		"typescript": "^3.8.3"
	},
	"scripts": {
		"tc-preinstall": "npm install --ignore-scripts && npm run build",
		"clean": "rm -rf dist && rm -rf tsconfig.build.tsbuildinfo",
		"copy-files": "copyfiles -u 1 \"src/**/*.{js,xml,template,json}\" dist",
		"lerna-init": "npm install --ignore-scripts && npm run build",
		"build": "npm run clean && npm run copy-files && npx tsc -b tsconfig.build.json",
		"test": "jest",
		"postinstall": "node dist/postinstall.js"
	}
}
