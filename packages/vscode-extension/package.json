{
	"name": "@oracle/suitecloud-vscode-extension",
	"displayName": "suitecloud-vscode-extension",
	"description": "netsuite suitecloud vscode extension based on nodejs-cli",
	"publisher": "Oracle",
	"private": true,
	"version": "0.0.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/oracle/netsuite-suitecloud-sdk",
		"directory": "packages/vscode-extension"
	},
	"files": [
		"**/*.vsix"
	],
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./dist/suitecloud.js",
	"contributes": {
		"commands": [
			{
				"command": "suitecloud.adddependencies",
				"title": "Add Dependency References to the Manifest",
				"category": "SuiteCloud"
			},
			{
				"command": "suitecloud.deploy",
				"title": "Deploy Project",
				"category": "SuiteCloud"
			},
			{
				"command": "suitecloud.listobjects",
				"title": "List Objects",
				"category": "SuiteCloud"
			},
			{
				"command": "suitecloud.uploadfile",
				"title": "Upload File",
				"category": "SuiteCloud"
			},
			{
				"command": "suitecloud.setupaccount",
				"title": "Setup Account",
				"category": "SuiteCloud"
			},
			{
				"command": "suitecloud.updateobject",
				"title": "Update Single Object with Account Object",
				"category": "SuiteCloud"
			},
			{
				"command": "suitecloud.importobject",
				"title": "Import Single Object to Account",
				"category": "SuiteCloud"
			},
			{
				"command": "suitecloud.compareobject",
				"title": "Compare Custom Object with Account copy",
				"category": "SuiteCloud"
			},
			{
				"command": "suitecloud.importfile",
				"title": "Import Single File to Account",
				"category": "SuiteCloud"
			},
			{
				"command": "suitecloud.comparefile",
				"title": "Compare Custom File with Account copy",
				"category": "SuiteCloud"
			}
		],
		"keybindings": [
			{
				"command": "suitecloud.adddependencies",
				"key": "ctrl+alt+shift+a",
				"mac": "ctrl+alt+shift+a"
			},
			{
				"command": "suitecloud.deploy",
				"key": "ctrl+alt+d",
				"mac": "ctrl+alt+d",
				"linux": "meta+alt+d"
			},
			{
				"command": "suitecloud.importobject",
				"key": "ctrl+alt+i",
				"mac": "ctrl+alt+i"
			},
			{
				"command": "suitecloud.compareobject",
				"key": "ctrl+alt+c",
				"mac": "ctrl+alt+c"
			},
			{
				"command": "suitecloud.compareobject",
				"key": "ctrl+alt+c",
				"mac": "ctrl+alt+c"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "resource =~ //Objects/.*\\.xml$|^file$/",
					"command": "suitecloud.importobject",
					"group": "suitecloud@1"
				},
				{
					"when": "resource =~ //Objects/.*\\.xml$|^file$/",
					"command": "suitecloud.compareobject",
					"group": "suitecloud@1"
				},
				{
					"when": "resource =~ //FileCabinet/SuiteScripts/.*\\.js$|^file$/",
					"command": "suitecloud.importfile",
					"group": "suitecloud@1"
				},
				{
					"when": "resource =~ /FileCabinet/SuiteScripts/.*\\.js$|^file$/",
					"command": "suitecloud.comparefile",
					"group": "suitecloud@1"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./"
	},
	"dependencies": {
		"@oracle/suitecloud-cli": "^1.1.1"
	},
	"devDependencies": {
		"@types/glob": "7.1.3",
		"@types/mocha": "8.2.0",
		"@types/node": "14.14.22",
		"@types/vscode": "1.52.0",
		"@typescript-eslint/eslint-plugin": "4.14.1",
		"@typescript-eslint/parser": "4.14.1",
		"eslint": "7.19.0",
		"eslint-config-prettier": "7.2.0",
		"glob": "7.1.6",
		"mocha": "8.2.1",
		"typescript": "4.1.3",
		"vscode-test": "1.5.0"
	}
}
